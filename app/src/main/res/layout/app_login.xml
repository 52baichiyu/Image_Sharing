<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent" android:layout_height="match_parent" android:orientation="vertical" tools:context=".login.App_Login" android:background="@color/white" android:alpha="1.5">
    <ImageView android:layout_width="match_parent" android:layout_height="match_parent" android:background="@drawable/app_fort1" android:alpha="0.6"> </ImageView>
    <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:orientation="vertical">
        <RelativeLayout android:id="@+id/sigicon" android:layout_width="match_parent" android:layout_height="361dp" android:alpha="0.8">

            <ImageView
                android:id="@+id/logo"
                android:layout_width="160dp"
                android:layout_height="160dp"
                android:layout_centerInParent="true"
                android:src="@drawable/logo" />
        </RelativeLayout>
        <RelativeLayout android:id="@+id/sigicon3" android:layout_width="match_parent" android:layout_height="50dp" tools:layout_editor_absoluteX="0dp" tools:layout_editor_absoluteY="561dp" android:alpha="0.8">
            <ImageView android:id="@+id/uers" android:layout_width="40dp" android:layout_height="40dp" android:layout_alignParentStart="true" android:layout_marginStart="55dp" android:src="@drawable/ic_baseline_person_outline_24"/>

            <EditText
                android:id="@+id/account"
                android:layout_width="220dp"
                android:layout_height="50dp"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="85dp"
                android:layout_toEndOf="@+id/uers"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
        </RelativeLayout>
        <RelativeLayout android:id="@+id/sigicon2" android:layout_width="match_parent" android:layout_height="50dp" tools:layout_editor_absoluteX="0dp" tools:layout_editor_absoluteY="1dp" android:layout_marginTop="10dp" android:alpha="0.8">

            <ImageView
                android:id="@+id/password"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="56dp"
                android:src="@drawable/ic_baseline_lock_24" />

            <EditText
                android:id="@+id/password1"
                android:layout_width="220dp"
                android:layout_height="50dp"
                android:layout_marginStart="16dp"
                android:layout_toEndOf="@+id/password"
                android:inputType="textPassword"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <ImageButton
                android:id="@+id/checkpassword"
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="85dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/ic_baseline_visibility_24"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
        </RelativeLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="40dp">

            <CheckBox
                android:id="@+id/checkBox"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="55dp"
                android:scaleX="0.9"
                android:scaleY="0.9"
                android:text="记住密码"
                android:textColor="#000000"
                tools:ignore="TouchTargetSizeCheck" />
        </RelativeLayout>
        <Button android:id="@+id/bt_login" android:layout_width="250dp" android:layout_height="50dp" android:layout_gravity="center" android:layout_marginTop="30dp" android:background="@drawable/my_button_login" android:text="登 录" android:textSize="20dp" android:textColor="@color/teal_700" tools:ignore="TouchTargetSizeCheck"/>
        <Button android:id="@+id/bt_Register" android:layout_width="250dp" android:layout_height="50dp" android:layout_gravity="center" android:layout_marginTop="30dp" android:background="@drawable/my_button_register" android:text="注 册" android:textSize="20dp" android:textColor="#548C8C" tools:ignore="TouchTargetSizeCheck"/>
    </LinearLayout>
</RelativeLayout>